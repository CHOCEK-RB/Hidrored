				{
					"name": "Crear un nuevo reporte",
					"event": [
						{
							"listen": "prerequest",
							"script": {
								"exec": [
									"function getRandomType() {",
									"    const typeEnum = [\"FUGA\", \"TUBERIA_ROTA\", \"DESBORDE\", \"OTROS\"];",
									"    const randomIndex = Math.floor(Math.random() * typeEnum.length);",
									"    return typeEnum[randomIndex];",
									"}",
									"",
									"function getRandomPriority() {",
									"    const priorityEnum = [\"BAJA\", \"MEDIA\", \"ALTA\"];",
									"    const randomIndex = Math.floor(Math.random() * priorityEnum.length);",
									"    return priorityEnum[randomIndex];",
									"}",
									"",
									"function getRandomLatitud() {",
									"    const minLat = -17.2983;",
									"    const maxLat = -14.6017;",
									"    const randomLat = Math.random() * (maxLat - minLat) + minLat;",
									"    return randomLat.toFixed(6);",
									"}",
									"",
									"function getRandomLongitud() {",
									"    const minLon = -81.3264;",
									"    const maxLon = -70.0978;",
									"    const randomLon = Math.random() * (maxLon - minLon) + minLon;",
									"    return randomLon.toFixed(6);",
									"}",
									"",
									"const tipoAleatorio = getRandomType();",
									"const prioridadAleatorio = getRandomPriority();",
									"const latitudAleatoria = getRandomLatitud();",
									"const longitudAleatoria = getRandomLongitud();",
									"",
									"pm.variables.set(\"tipo\", tipoAleatorio);",
									"pm.variables.set(\"prioridad\", prioridadAleatorio);",
									"pm.variables.set(\"latitud\", latitudAleatoria);",
									"pm.variables.set(\"longitud\", longitudAleatoria);",
									"",
									"console.log(\"Tipo seleccionado:\", tipoAleatorio);",
									"console.log(\"Prioridad seleccionada:\", prioridadAleatorio);",
									"console.log(\"Latitud generada:\", latitudAleatoria);",
									"console.log(\"Longitud generada:\", longitudAleatoria);"
								],
								"type": "text/javascript",
								"packages": {},
								"requests": {}
							}
						},
